{
  "@context": {
    "rdf": "https://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "https://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "webiot": "https://schema.iot.webizing.org/",
    "xsd": "https://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "webiot:url",
      "@type": "rdf:Property",
      "rdfs:comment": "link to the video",
      "rdfs:label": "    url",
      "schema:rangeIncludes": {
        "@id": "schema:URL"
      }
    },
    {
      "@id": "webiot:location",
      "@type": "rdf:Property",
      "rdfs:comment": "location of the sensor - format: free",
      "rdfs:label": "location",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:address",
      "@type": "rdf:Property",
      "rdfs:comment": "building where the sensor resides - format: organization-building ex)yongjaelee-house, kist-l1, kist-l8",
      "rdfs:label": "address",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:name",
      "@type": "rdf:Property",
      "rdfs:comment": "name of the sensor - format: SensorXX ex)Foobot00, Beddit02",
      "rdfs:label": "name",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:user",
      "@type": "rdf:Property",
      "rdfs:comment": "user of the sensor - format: firstName+LastName ex)jonghoLee, wanhoIm",
      "rdfs:label": "user",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:hash",
      "@type": "rdf:Property",
      "rdfs:comment": "hash to the video file in the IPFS system",
      "rdfs:label": "hash",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:IPFSCamera",
      "@type": "rdfs:Class",
      "rdfs:comment": "IPFSCamera Sensor - video content from the IPFS Camera",
      "rdfs:label": "IPFSCamera",
      "schema:sameAs": {
        "@id": "webiot:IPFSCamera"
      }
    },
    {
      "@id": "webiot:time",
      "@type": "rdf:Property",
      "rdfs:comment": "time at which the data measurement was made - ex)2017-05-30T18:54:20+09:00",
      "rdfs:label": "time",
      "schema:rangeIncludes": {
        "@id": "schema:DateTime"
      }
    },
    {
      "@id": "webiot:room",
      "@type": "rdf:Property",
      "rdfs:comment": "room number of the building where the sensor resides - ex) L8321",
      "rdfs:label": "room",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    }
  ]
}