{
  "@context": {
    "rdf": "https://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "https://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "webiot": "https://schema.iot.webizing.org/",
    "xsd": "https://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "webiot:voc",
      "@type": "rdf:Property",
      "rdfs:comment": "휘발성 유기 화합물 - unit: ppb",
      "rdfs:label": "voc",
      "schema:rangeIncludes": {
        "@id": "schema:Float"
      }
    },
    {
      "@id": "webiot:user",
      "@type": "rdf:Property",
      "rdfs:comment": "user of the sensor - format: firstName+LastName ex)jonghoLee, wanhoIm",
      "rdfs:label": "user",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:address",
      "@type": "rdf:Property",
      "rdfs:comment": "building where the sensor resides - format: organization-building ex)yongjaelee-house, kist-l1, kist-l8",
      "rdfs:label": "address",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:humidity",
      "@type": "rdf:Property",
      "rdfs:comment": "습도 - unit: %",
      "rdfs:label": "humidity",
      "schema:rangeIncludes": {
        "@id": "schema:Float"
      }
    },
    {
      "@id": "webiot:AirQuality",
      "@type": "rdfs:Class",
      "rdfs:comment": "AirQuality Sensor - 공기질 로깅 ex)Foobot(5분 주기로 데이터를 가져옴)",
      "rdfs:label": "AirQuality",
      "schema:sameAs": {
        "@id": "webiot:AirQuality"
      }
    },
    {
      "@id": "webiot:particle",
      "@type": "rdf:Property",
      "rdfs:comment": "pm2.5 - unit: ㎍/㎥",
      "rdfs:label": "particle",
      "schema:rangeIncludes": {
        "@id": "schema:Float"
      }
    },
    {
      "@id": "webiot:location",
      "@type": "rdf:Property",
      "rdfs:comment": "location of the sensor - format: free",
      "rdfs:label": "location",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:temp",
      "@type": "rdf:Property",
      "rdfs:comment": "temperature - unit: Celsius(℃)",
      "rdfs:label": "temp",
      "schema:rangeIncludes": {
        "@id": "schema:Float"
      }
    },
    {
      "@id": "webiot:pollution",
      "@type": "rdf:Property",
      "rdfs:comment": "미세먼지 - unit: TBD",
      "rdfs:label": "pollution",
      "schema:rangeIncludes": {
        "@id": "schema:Float"
      }
    },
    {
      "@id": "webiot:room",
      "@type": "rdf:Property",
      "rdfs:comment": "room number of the building where the sensor resides - ex) L8321",
      "rdfs:label": "room",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:co2",
      "@type": "rdf:Property",
      "rdfs:comment": "이산화탄소 - unit: ppm",
      "rdfs:label": "co2",
      "schema:rangeIncludes": {
        "@id": "schema:Float"
      }
    },
    {
      "@id": "webiot:time",
      "@type": "rdf:Property",
      "rdfs:comment": "time at which the data measurement was made - ex)2017-05-30T18:54:20+09:00",
      "rdfs:label": "time",
      "schema:rangeIncludes": {
        "@id": "schema:DateTime"
      }
    },
    {
      "@id": "webiot:name",
      "@type": "rdf:Property",
      "rdfs:comment": "name of the sensor - format: SensorXX ex)Foobot00, Beddit02",
      "rdfs:label": "name",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    }
  ]
}
